{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@sections/*": ["src/sections/*"],
      "@scripts/*": ["src/scripts/*"],
      "@data/*": ["src/data/*"],
      "@icons/*": ["src/icons/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
